                    "Products"
                ],
                "summary": "Update a product",
                "description": "Update a product",
                "operationId": "1419d479f9340e30e67fe21a2ca525b7",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "Product ID",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "name": {
                                        "description": "The name of the product",
                                        "type": "string",
                                        "example": "Coca Cola"
                                    },
                                    "api_id": {
                                        "description": "External API ID for the product",
                                        "type": "string",
                                        "example": "1234",
                                        "nullable": true
                                    },
                                    "image": {
                                        "description": "URL of the product image",
                                        "type": "string",
                                        "example": "http://example.com/image.png",
                                        "nullable": true
                                    },
                                    "country": {
                                        "description": "Country of origin of the product",
                                        "type": "string",
                                        "example": "France",
                                        "nullable": true
                                    },
                                    "brand": {
                                        "description": "The brand of the product",
                                        "type": "string",
                                        "example": "Coca Cola Company",
                                        "nullable": true
                                    },
                                    "description": {
                                        "description": "Description of the product",
                                        "type": "string",
                                        "example": "A refreshing soda.",
                                        "nullable": true
                                    },
                                    "category": {
                                        "description": "Category of the product",
                                        "type": "string",
                                        "example": "Beverages",
                                        "nullable": true
                                    },
                                    "tags": {
                                        "description": "Tags associated with the product",
                                        "type": "string",
                                        "example": "soda,beverage,refreshing",
                                        "nullable": true
                                    },
                                    "ingredients": {
                                        "description": "Ingredients of the product",
                                        "type": "string",
                                        "example": "Water, Sugar, Carbon Dioxide",
                                        "nullable": true
                                    },
                                    "serving_size_unit": {
                                        "description": "Unit of the serving size",
                                        "type": "string",
                                        "example": "ml",
                                        "nullable": true
                                    },
                                    "serving_size": {
                                        "description": "Size of one serving",
                                        "type": "number",
                                        "format": "float",
                                        "example": 330,
                                        "nullable": true
                                    },
                                    "calories": {
                                        "description": "Calories per serving",
                                        "type": "number",
                                        "format": "float",
                                        "default": 0,
                                        "example": 139
                                    },
                                    "fat": {
                                        "description": "Fat content per serving",
                                        "type": "number",
                                        "format": "float",
                                        "default": 0,
                                        "example": 0
                                    },
                                    "carbohydrates": {
                                        "description": "Carbohydrates per serving",
                                        "type": "number",
                                        "format": "float",
                                        "default": 0,
                                        "example": 35
                                    },
                                    "protein": {
                                        "description": "Protein content per serving",
                                        "type": "number",
                                        "format": "float",
                                        "default": 0,
                                        "example": 0
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Product updated successfully"
                    }
                }
            },
